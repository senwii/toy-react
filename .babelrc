{
    "presets": [
        ["@babel/preset-env", {
            "targets": {
                "browsers": "last 2 Chrome versions"
            }
        }],
        "@babel/preset-react"
    ],
    "plugins": [
        ["@babel/plugin-transform-react-jsx", {
            "pragma": "ToyReact.createVdom",
            "pargmaFrag": "ToyReact.Fragment",
            "throwIfNamespace": false
        }]
    ]
}